{
  "name": "chatterbox2",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && npm run build:copy-assets",
    "build:copy-assets": "copyfiles src/assets/* src/assets/**/* --up 2 --exclude **/*.ts dist/assets/",
    "test": "tsc --noEmit",
    "prestart": "npm run build",
    "start": "twilio-run --functions-folder dist/functions --assets-folder dist/assets",
    "predeploy": "npm run build",
    "deploy": "twilio-run deploy --functions-folder dist/functions --assets-folder dist/assets --runtime node14 --override-existing-project",
    "promote": "twilio-run promote --to= --from=dev"
  },
  "dependencies": {
    "@twilio-labs/serverless-runtime-types": "^2.2.3"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "prettier": "2.7.1",
    "twilio-run": "^3.4.2",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "14"
  }
}
